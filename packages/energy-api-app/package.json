{
  "name": "@energyweb/energy-api-app",
  "version": "0.1.0",
  "description": "An example hosted API using @energyweb/energy-api",
  "author": "EnergyWeb DevHub GmbH; Piotr Kosinski, piotr.kosinski@energyweb.org",
  "license": "GPL-3.0-or-later",
  "main": "dist/index.js",
  "scripts": {
    "precommit": "lint-staged",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@energyweb/energy-api-influxdb": "0.5.0",
    "@nestjs/common": "7.4.2",
    "@nestjs/config": "0.5.0",
    "@nestjs/core": "7.4.4",
    "@nestjs/swagger": "4.6.1",
    "@nestjs/platform-express": "7.5.5",
    "class-transformer": "0.2.3",
    "class-validator": "0.12.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.3",
    "swagger-ui-express": "4.1.4",
    "json-to-pretty-yaml": "1.2.2"
  },
  "devDependencies": {
    "@nestjs/cli": "7.5.1",
    "@nestjs/schematics": "7.1.1",
    "@types/express": "4.17.7",
    "@types/node": "13.1.6",
    "@typescript-eslint/eslint-plugin": "4.2.0",
    "@typescript-eslint/parser": "4.2.0",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-import": "2.22.1",
    "lint-staged": "10.4.0",
    "prettier": "2.1.2",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
  },
  "files": [
    "dist"
  ],
  "private": "true"
}
